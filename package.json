{
  "name": "cozy-bookmarks",
  "version": "0.0.1",
  "engines": [
    "node = 0.8.x"
  ],
  "description": "Manage your bookmark from your Cozy!",
  "repository": {
    "type": "git",
    "url": "https://github.com/mycozycloud/cozy-template.git"
  },
  "main": "server.coffee",
  "dependencies": {
    "americano": "~0.2.5",
    "americano-cozy": "~0.1.2"
  },
  "devDependencies": {
    "should": "= 1.2.0",
    "mocha": "= 1.6.0",
    "chai": "= 1.3.0"
  },
  "scripts": {
    "test": "cake tests",
    "start": "node server.js"
  },
  "cozy-displayName": "Bookmarks",
  "cozy-permissions": {
    "bookmark": {
      "description": "Allow you to manage your bookmarks"
    }
  }
}
